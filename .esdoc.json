{
  "source": ".",
  "destination": "./docs",
  "plugins": [
    {
      "name": "esdoc-standard-plugin",
      "option": {
        "test": {
          "source": ".",
          "includes": [
            "^index.test.js$"
          ]
        }
      }
    },
    {
      "name": "esdoc-importpath-plugin",
      "option": {
        "replaces": [
          {"from": "^.+$", "to": ""}
        ]
      }
    }
  ],
  "includes": [
    "^index.js$"
  ]
}
